@model GiftViewModel
@{ 
    ViewBag.Title = "Create Gift";
}

<form asp-controller="Gifts" asp-action="Create" method="post">
    @await Html.PartialAsync("_CreateOrEdit")
    <button class="btnConfirm">Create</button>
    <a class="btnCancel" asp-controller="Gifts" asp-action="Index">Cancel</a>
</form>
